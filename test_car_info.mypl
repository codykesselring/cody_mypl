class Car{
    string brand;   
    string model;
    int year;
    void print_contents(){
        print("Brand: ");
        print(this.brand);
        print("\nModel: ");
        print(this.model);
        print("\nYear: ");
        print(this.year);
    }
}

void main(){
    Car carObj = new Car("BMW", "X5", 1999);
    carObj.brand = "BMW";
    carObj.print_contents();
}